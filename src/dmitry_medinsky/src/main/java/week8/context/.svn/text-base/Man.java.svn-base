public class Man {

    private String name = null;
    private Integer age = null;
    private Firm firm = null;

    public Man() {

    }

    public Man(String name, Integer age, Firm firm) {
        this.name = name;
        this.age = age;
        this.firm = firm;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public Integer getAge() {
        return age;
    }

    public void setAge(Integer age) {
        this.age = age;
    }

    public Firm getFirm() {
        return firm;
    }

    public void setFirm(Firm firm) {
        this.firm = firm;
    }

    @Override
    public String toString() {
        return "Человек{" +
                "имя='" + name + '\'' +
                ", возраст=" + age +
                '}';
    }
}
