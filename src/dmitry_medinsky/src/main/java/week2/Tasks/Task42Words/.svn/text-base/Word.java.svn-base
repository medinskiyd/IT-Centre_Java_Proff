public class Word implements Comparable{

    private String str = "";

    public Word () {
    }

    public Word (String str) {
        this.str = str;
    }

    public String getStr() {
        return str;
    }

    public void setStr(String str) {
        this.str = str;
    }

    @Override
    public int compareTo(Object o) {

        Word word = (Word)o;

        int a = word.getStr().compareTo(str);

        return a;
    }
}
