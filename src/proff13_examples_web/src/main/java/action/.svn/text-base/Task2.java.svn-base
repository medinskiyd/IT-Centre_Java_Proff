public class Task2 {
    public Task2()
    {
    }
    public ArrayList<Integer> add10(ArrayList<Integer> listin){
        ArrayList<Integer> list = new ArrayList<>();
        for (int i=0; i<listin.size(); i++)
        {
            list.add(listin.get(i)+10);

        }
        return list;
    }
}
